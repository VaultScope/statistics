{
  "name": "@vaultscope/statistics-server",
  "version": "1.0.0",
  "description": "VaultScope Statistics monitoring server - Real-time system monitoring API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch '*.ts' --exec 'ts-node' index.ts",
    "build": "tsc --noEmitOnError false",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "apikey": "ts-node cli/apikey.ts",
    "speed": "ts-node __tests__/speedtest.ts",
    "sysinfo": "ts-node __tests__/sysinfo.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rm -rf dist node_modules"
  },
  "keywords": [
    "monitoring",
    "server",
    "api",
    "system",
    "statistics",
    "metrics",
    "realtime"
  ],
  "author": "VaultScope",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.2.0",
    "body-parser": "^1.20.2",
    "commander": "^12.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "drizzle-orm": "latest",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "jose": "^5.2.0",
    "morgan": "^1.10.1",
    "ps-list": "^8.1.1",
    "systeminformation": "^5.21.20",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "drizzle-kit": "latest",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "cap": "^0.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
