  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "ts-node ./server/db/migrate.ts",
    "db:push": "drizzle-kit push:sqlite",
    "db:studio": "drizzle-kit studio",
    "build:server": "tsc",
    "build:client": "cd client && next build",
    "build:all": "npm run build:server && npm run build:client",
    "speed": "ts-node ./server/__tests__/speedtest.ts",
    "sysinfo": "ts-node ./server/__tests__/sysinfo.ts",
    "apikey": "ts-node ./server/cli/apikey.ts",
    "server": "ts-node ./server/index.ts",
    "server:dev": "ts-node ./server/index.ts",
    "server:prod": "NODE_ENV=production node dist/server/index.js",
    "client": "cd client && next dev -p 4001",
    "client:build": "cd client && next build",
    "client:dev": "cd client && next dev -p 4001",
    "client:prod": "cd client && NODE_ENV=production next start -p 4001",
    "start": "NODE_ENV=production npm run server:prod",
    "start:all": "NODE_ENV=production concurrently \"npm run server:prod\" \"npm run client:prod\"",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "deploy": "npm run build:all && npm run start:all"
  },
